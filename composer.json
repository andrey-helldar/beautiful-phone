{
    "name": "andrey-helldar/beautiful-phone",
    "description": "Formatting a phone number into a beautiful view",
    "type": "library",
    "license": "MIT",
    "keywords": ["beautiful-phone", "beautiful", "phone", "formatter"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0|^7.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\BeautifulPhone\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\BeautifulPhone\\ServiceProvider"
            ]
        }
    }
}
